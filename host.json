{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensionBundle": {
    "id": "Microsoft.Azure.Functions.ExtensionBundle",
    "version": "[4.*, 5.0.0)"
  },
  "fileWatchingEnabled": false,
  "customHandler": {
    "description": {
      "defaultExecutablePath": "/home/site/wwwroot/.deno/handler.sh",
      "workingDirectory": "/home/site/wwwroot"
    },
    "enableForwardingHttpRequest": true
  }
}